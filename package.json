{
  "name": "decentraland-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/graphprotocol/decentraland-subgraph",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "create-local": "graph create graphprotocol/decentraland --node http://127.0.0.1:8020",
    "build": "graph build",
    "deploy-local": "graph deploy graphprotocol/decentraland --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020/",
    "deploy": "graph deploy --debug graphprotocol/decentraland --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "watch-local": "graph deploy graphprotocol/decentraland --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.13.1",
    "@graphprotocol/graph-ts": "^0.13.0"
  }
}
